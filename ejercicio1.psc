Algoritmo suma_multiplicacion
	escribir " ingrese el primer numero "
	leer num1;
	escribir " ingrese el segundo numero "
	leer num2;
	escribir " ingrese el segundo numero "
	leer num3;
	C <- num1 + num2 + num3;
	M <- num1 * num2 * num3;
	escribir " la suma es: " ,C;
	escribir " la multiplicacion es: " ,M
FinAlgoritmo
